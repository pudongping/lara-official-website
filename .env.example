APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
# laravel-debugbar
DEBUGBAR_ENABLED=false
APP_URL=http://localhost

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# 生成的 token 在多少分钟后过期，默认 60 分钟。这里设置为 10080 分钟，一星期。
JWT_TTL=10080
# 生成的 token，在多少分钟内，可以刷新获取一个新 token，默认 20160 分钟，14 天。这里设置为 43200 分钟，30 天
JWT_REFRESH_TTL=43200
# 登录时选择「记住我」时，token 的有效期，自定义设置为 10080 分钟，一星期。
JWT_REMEMBER_ME_TTL=10080
# 设定 JWT 的宽限时间，单位：秒
JWT_BLACKLIST_GRACE_PERIOD=60
# 设置距离多少秒后自动刷新 token 这里设置为 300 秒，5 分钟
JWT_AUTO_REFRESH_TIME=300
